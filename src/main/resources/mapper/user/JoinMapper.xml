<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jjobkorea.mapper.user.JoinMapper">


<sql id="joinColumns">
	id
	,userId
	,passWord
</sql>

<insert id="insertJoin" parameterType="JoinDTO">
		insert into team_db.join (id,userId,passWord) values (#{id},#{userId},#{passWord});
</insert>


</mapper>


